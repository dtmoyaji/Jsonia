{
  "state": {
    "componentsData": [],
    "iconMap": {},
    "extensions": null,
    "selectedElement": null,
    "selectedTab": "structure",
    "config": null,
    "history": [],
    "historyIndex": -1,
    "draggedComponent": null,
    "dragStartActions": [
      { "type": "function", "name": "handleDragStart" }
    ],
    "innerDropActions": [
      { "type": "function", "name": "handleInnerDrop" }
    ]
  },
  "computed": {},
  "apis": {
    "loadComponents": { "url": "/editor/api/components", "method": "GET" },
    "loadBuiltinComponents": { "url": "/editor/data/components.json", "method": "GET" },
    "loadEditorComponents": { "url": "/editor/api/editor-components", "method": "GET" },
    "loadExtensions": { "url": "/editor/extensions.json", "method": "GET" },
    "loadConfig": { "url": "/editor/config.json", "method": "GET" }
  },
  "initialization": [
    { "type": "api", "name": "loadComponents", "storeIn": "sharedComponentsResponse" },
    { "type": "api", "name": "loadBuiltinComponents", "storeIn": "builtinComponentsResponse" },
    { "type": "setState", "key": "sharedComponents", "value": "{{sharedComponentsResponse.components}}" },
    { "type": "setState", "key": "componentsData", "value": "{{builtinComponentsResponse.components}}" },
    { "type": "setState", "key": "iconMap", "value": "{{builtinComponentsResponse.iconMap}}" },
    { "type": "registerComponentMethods", "components": "{{sharedComponents}}" },
    { "type": "api", "name": "loadEditorComponents", "storeIn": "editorComponentsResponse" },
    { "type": "setState", "key": "editorComponents", "value": "{{editorComponentsResponse.components}}" },
    { "type": "registerComponentActions", "components": "{{editorComponents}}" },
    { "type": "api", "name": "loadExtensions", "storeIn": "extensionsResponse" },
    { "type": "setState", "key": "extensions", "value": "{{extensionsResponse}}" },
    { "type": "registerExtensions" },
    { "type": "api", "name": "loadConfig", "storeIn": "configResponse" },
    { "type": "setState", "key": "config", "value": "{{configResponse}}" },
    { "type": "function", "name": "renderComponentsList" },
    { "type": "function", "name": "updateStructureTree" }
  ],
  "events": [
    {
      "target": ".editor-tab",
      "type": "click",
      "actions": [
        { "type": "function", "name": "switchTab" }
      ]
    },
    {
      "target": ".accordion-header",
      "type": "click",
      "actions": [
        { "type": "function", "name": "toggleAccordion" }
      ]
    },
    {
      "target": "[data-action]",
      "type": "click",
      "actions": [
        { "type": "function", "name": "handleAction" }
      ]
    },
    {
      "target": ".component-item",
      "type": "dragstart",
      "actions": [
        { "type": "function", "name": "handleDragStart" }
      ]
    },
    {
      "target": ".component-item",
      "type": "dragend",
      "actions": [
        { "type": "function", "name": "handleDragEnd" }
      ]
    },
    {
      "target": "#drop-zone",
      "type": "dragover",
      "actions": [
        { "type": "function", "name": "handleDragOver" }
      ]
    },
    {
      "target": "#drop-zone",
      "type": "drop",
      "actions": [
        { "type": "function", "name": "handleDrop" }
      ]
    },
    {
      "target": ".delete-component-btn",
      "type": "click",
      "actions": [
        { "type": "function", "name": "handleDeleteComponent" }
      ]
    }
  ]
}
